# JAVA INSTALLATION
sudo add-apt-repository ppa:openjdk-r/ppa
sudo apt-get update
sudo apt-get install openjdk-8-jdk -y

#create a directory

mkdir ui api

#ui git clone

cd ui

git clone=https://github.com/MadhaviB34/sms-ui.git

cd 

#api clone

cd api

git clone=https://github.com/MadhaviB34/sms-api.git
